{
    "plugins": ["prettier"],
    "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
    "env": {
        "es6": true,
        "browser": true,
        "jest": true,
        "node": true
    },
    "rules": {
        "react-hooks/exhaustive-deps": "off",
        "no-unused-vars": "off",
        "prettier/prettier": "warn",
        "react/display-name": "warn"
    }
}
